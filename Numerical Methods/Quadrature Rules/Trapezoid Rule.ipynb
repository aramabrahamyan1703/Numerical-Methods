   "source": [
    "def TrapezoidRule(f, a, b, n):\n",
    "    area = 0\n",
    "    h = (b-a)/n # length of sub-intervals\n",
    "    for i in range(n): # any stopping condition here\n",
    "        area += (f(a+i*h) + f(a+(i+1)*h))/2 # trapezoid rule formula\n",
    "    return area * h"
   ]
  }
